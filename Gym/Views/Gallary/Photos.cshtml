@model IndexViewModel
@{
    ViewData["Title"] = "Photos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="our_section layout_padding">
    <div class="container">
        <div class="row mt-3" style="min-height:600px;">
            @foreach (var group in Model.Group.Images)
            {
                <div class="col-md-3">
                    <a href="~/images/images/@group.photo" target="_blank">
                        <img src="~/images/images/@group.photo" style="width:100%; height:250px; object-fit:contain;" />
                    </a>
                </div>
            }
        </div>
    </div>
</div>
