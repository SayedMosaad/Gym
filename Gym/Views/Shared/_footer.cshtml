@model IndexViewModel

@for (int i = 0; i < Model.Biographies.Count; i++)
{
    if (i == 0)
    {
        <div class="row map_addres">
            <div class="col-sm-12 col-lg-4">
                <div class="map_text"><img src="/User/images/map-icon.png"><span class="map_icon">@Model.Biographies[0].Address</span></div>
            </div>
            <div class="col-sm-12 col-lg-4">
                <div class="map_text"><img src="/User/images/phone-icon.png"><span class="map_icon">( @Model.Biographies[0].Mobile )</span></div>
            </div>
            <div class="col-sm-12 col-lg-4">
                <div class="map_text"><img src="/User/images/email-icon.png"><span class="map_icon">@Model.Biographies[0].Email</span></div>
            </div>
        </div>
        <div class="social_icon">
            <ul>
                <li><a href="@Model.Biographies[0].Facebook"><img src="/User/images/fb-icon.png"></a></li>
                <li><a href="@Model.Biographies[0].Twitter"><img src="/User/images/twitter-icon.png"></a></li>
                <li><a href="@Model.Biographies[0].Instagram"><img src="/User/images/instagram-icon.png"></a></li>
            </ul>
        </div>
    }
}